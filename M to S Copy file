scp /etc/skel/.bash_profile $i:/etc/skel/
scp /etc/skel/.bashrc $i:/etc/skel/
scp /etc/bashrc $i:/etc/
scp /root/.bash_profile $i:/root/
scp /root/.bashrc $i:/root/
scp /etc/hosts $i:/etc/
scp /etc/passwd $i:/etc/
scp /etc/shadow $i:/etc/
scp /etc/group $i:/etc/
scp /etc/gshadow $i:/etc/
scp /etc/passwd- $i:/etc/
scp /etc/shadow- $i:/etc/
scp /etc/group- $i:/etc/
scp /etc/gshadow- $i:/etc/
scp /usr/share/Modules/init/.modulespath $i:/usr/share/Modules/init/
scp /etc/profile.d/hanul.sh $i:/etc/profile.d
scp /etc/profile.d/hanul.csh $i:/etc/profile.d
scp /etc/profile.d/license_* $i:/etc/profile.d
#scp /etc/environment $i:/etc
scp /etc/sudoers $i:/etc
ssh $i setenforce 0
scp /etc/selinux/config $i:/etc/selinux/
scp /etc/sysctl.d/hanul.conf $i:/etc/sysctl.d
#scp /etc/ld.so.conf $i:/etc
scp /etc/security/limits.conf $i:/etc/security
scp /etc/rsyslog.d/ignore-systemd-session-slice.conf $i:/etc/rsyslog.d
scp /usr/bin/collect_report.sh $i:/usr/bin/
ssh $i ldconfig